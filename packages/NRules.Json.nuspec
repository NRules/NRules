    <file src="..\binaries\NRules\netstandard2.0\NRules.Json.pdb" target="lib\netstandard2.0" />
    <file src="..\binaries\NRules\netstandard2.1\NRules.Json.dll" target="lib\netstandard2.1" />
    <file src="..\binaries\NRules\netstandard2.1\NRules.Json.xml" target="lib\netstandard2.1" />
